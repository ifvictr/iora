{
  "name": "iora-server",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "main": "./build/index.js",
  "scripts": {
    "build": "yarn clean && tsc",
    "clean": "rm -rf ./build && mkdir ./build",
    "dev": "nodemon -r ./node_modules/dotenv/config --watch './src/**/*.ts' --exec ts-node ./src/index.ts",
    "start": "yarn build && node ./build/index.js"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/node": "^14.0.27",
    "@types/socket.io": "^2.1.11",
    "dotenv": "^8.2.0",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
